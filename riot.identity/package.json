{
  "name": "riot.identity",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "jqbootswatchbundle": "cpx node_modules/webpack2-bootstrap3-jquery3/dist/**/*   ../src/TheWebApp/wwwroot/dist/externals/JQB  -v",
    "json": "cpx ./app/*.json                         ../src/TheWebApp/wwwroot/dist       -v",
    "html": "cpx ./app/*.html                         ../src/TheWebApp/wwwroot/dist       -v",
    "fonts": "cpx node_modules/bootstrap/fonts/**/*   ../src/TheWebApp/wwwroot/dist/fonts  -v",
    "postBuild": "npm-run-all html json fonts jqbootswatchbundle",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "build:dev-typicode": "webpack --env=dev-typicode --progress --profile --colors",
    "build:prod-typicode": "webpack --env=prod-typicode --progress --profile --colors",
    "dev:0": "npm-run-all dev-typicode build:dev postBuild",
    "dev": "npm-run-all dev:0 serve",
    "serve": "webpack-dev-server --env=dev --hot",
    "dev-typicode": "npm-run-all build:dev-typicode",
    "prod-typicode": "npm-run-all build:prod-typicode",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "P7-riotjs-host": "git+https://github.com/ghstahl/P7-riotjs-host.git",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "bootstrap": "^3.3.7",
    "bootstrap-validator": "^0.11.9",
    "bootswatch": "^3.3.7",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "jquery": "^3.2.1",
    "js-cookie": "^2.1.4",
    "loader-utils": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "nprogress": "^0.2.0",
    "riot": "^3.6.0",
    "riot-form": "^0.2.0",
    "riot-route": "^3.1.1",
    "riot-tag-loader": "^1.0.0",
    "riotcontrol": "0.0.3",
    "riotjs-loader": "^4.0.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack2-bootstrap3-jquery3": "git+https://github.com/ghstahl/webpack2-bootstrap3-jquery3.git",
    "whatwg-fetch": "^2.0.3"
  }
}
